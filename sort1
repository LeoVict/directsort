#include <iostream>

using namespace std;
void directsort(int a[],int n){
        int temp,i,j;
        for (i=1;i<n;i++){
            if (a[i]<a[i-1]){
                temp=a[i];
                for(j=i-1;j>=0&&a[j]>=temp;j--){
                    a[j+1]=a[j];
                }
                a[j+1]=temp;
            }
        }
}//直接插入排序算法

void insertsort(int a[],int n){
        int temp,i,j,low,high,mid;
        for (i=1;i<n;i++){
                temp=a[i];
            low=0;high=i-1;
            while(low<=high){
                mid=(low+high)/2;
                if(a[mid]>temp)
                    high=mid-1;
                else low=mid+1;
            }
                for(j=i-1;j>=high+1;j--){
                    a[j+1]=a[j];
                }
                a[high+1]=temp;
            }
}//折半插入排序算法

void print (int a[6],int n){
        for (int i=0;i<n;i++){
            cout<<a[i]<<" ";
        }
        cout<<endl;
}
int main(){
    int a[6]={6,5,4,3,2,1};
    /*directsort(a,6)*/
    /*insertsort(a,6)*/
    ;
    print(a,6);
}
